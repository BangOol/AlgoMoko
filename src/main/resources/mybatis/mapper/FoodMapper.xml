<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.co.algomoko.food.mapper.FoodMapper">

	<!-- 음식 검색 결과 리스트 -->
	<select id="fList" resultType="com.co.algomoko.food.domain.FoodVO">
		select ing, cal, amount, F0
		from food
		where ing like '%' || #{ing} || '%'		
		order by ing
	</select>
	<!--  음식 검색 결과 단건 조회-->
	<select id="fListOne" resultType="com.co.algomoko.food.domain.FoodVO">
		select ing, cal, carb, prot, fat, F0
		from food
		where ing = ?
	</select>
</mapper>